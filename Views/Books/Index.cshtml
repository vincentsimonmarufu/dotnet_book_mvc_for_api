@model IEnumerable<BookMvcApp.Models.Book>

<h1>Books</h1>

<table class="table">
    <thead>
    <tr>
        <th>Title</th>
        @* <th>Author</th> *@
    </tr>
    </thead>
    <tbody>
    @foreach (var book in Model)
    {
        <tr>
            <td>@book.Title</td>
            @* <td>@book.Author.Name</td> *@
        </tr>
    }
    </tbody>
</table>